{
    "name": "DTF Extension",
    "description": "Extract all text from current web page and perform analysis",
    "version": "1.0.1",
    "manifest_version": 3,
    "icons": {
        "16":"icon.png",
        "32":"icon.png",
        "48":"icon.png",
        "128":"icon.png"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": ["scripting", "activeTab"],
    "host_permissions": [
        "*://*/*"
      ],
    "background":{}
}